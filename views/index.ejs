                                        </div><!-- end media -->
                                    </div><!-- end col -->

                                    <div class="blog-meta big-meta col-md-8">
                                        <h4><a href="/single?id=<%=data[i].id%>" title=""><%=(data[i].title)%></a></h4>
                                        <p><%=data[i].short%></p>
                                        <small class="firstsmall"><a class="bg-orange" href="/gadget" title=""><%=data[i].tag%></a></small>
